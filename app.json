{
  "expo": {
    "name": "Ritual",
    "slug": "Ritual",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/favicon-32x32.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/android-chrome-512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#f7efe2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android-chrome-192x192.png",
        "backgroundColor": "#f7efe2"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/favicon-16x16.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
